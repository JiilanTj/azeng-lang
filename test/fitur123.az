// Test fungsi dengan parameter dan return values
fungsi_int tambah(x: int, y: int) {
    kembali x + y;
}

fungsi_float bagi(a: float, b: float) {
    kembali a / b;
}

fungsi_bool lebih_besar(n1: int, n2: int) {
    kembali n1 > n2;
}

fungsi_str gabung(kata1: str, kata2: str) {
    kembali kata1 + kata2;
}

bikin fungsi main() {
    // Test integer
    isi hasil = tambah(10, 5);
    cetak("Hasil tambah: ");
    cetak(hasil);
    
    // Test float
    isi pembagian = bagi(15.5, 2.0);
    cetak("Hasil bagi: ");
    cetak(pembagian);
    
    // Test boolean
    isi perbandingan = lebih_besar(10, 5);
    cetak("10 lebih besar dari 5?: ");
    cetak(perbandingan);
    
    // Test string
    isi kata = gabung("Hello ", "World!");
    cetak("Hasil gabung: ");
    cetak(kata);
} 